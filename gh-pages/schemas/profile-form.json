{
    "html":[
          {
            "type":"fieldset",
            "id": "profile-form",
            "html":[            
                {
                    "name":"title",
                    "caption":"<h3>How may we call you</h3>",
                    "type":"text",
                    "id": "player_name"
                },

                {
                    "name":"title",
