                        "San Jose" : "San Jose",
                        "San Francisco" : "San Francisco",
                        "Oakland" : "Oakland",
                        "Berkeley" : "Berkeley"
                      }
                },                
                {
                    "name":"zipcode",
                    "caption":"<h3>Zipcode</h3>What's your zipcode?",
                    "type":"text",
                    "id": "zipcode",
                    "validate" : {
                        "required" : true,
                        "validZipcode": true,
                        "minlength" : 5,
                        "maxlength" : 5,
                        "messages" : {
                            "required" : "Required input"
                        }
                    }
                },
                {
                    "name":"lat",
                    "id":"lat",
